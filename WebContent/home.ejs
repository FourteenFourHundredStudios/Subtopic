<html>
    <head>
        <title>SubTopic: Home</title>
        <link rel="stylesheet" type="text/css" href="/static/Card.css"/>
        <link rel="stylesheet" type="text/css" href="/static/Elements.css"/>
        <script src="/static/jquery.js"></script>
        <script type="text/javascript" src="/static/util.js"></script>

       

    </head>
    <body>

        <div style="margin:0px;top:0;left:0;width:100%;padding:0px;background-color:#42A5F5;position:fixed;">
            <h1 style="padding:10px;color:white;padding:0px;padding-left:30px;float:left">[Subtopic]</h1>
            <h3 style="padding:10px;color:white;padding:0px;padding-right:30px;float:right;padding-top:15px">Logout</h3>
                
        </div>

        <br><br><br><br>

        <div style="padding:30px">
          

            <div id="leftPane" style="float:left;width:30%"> 

                <div id="loginForm">
                    <h1>Login</h1>
                    <div class="card">
                        <div class="cardBody">
                            <input class="textbox" id="username" placeholder="username" type="text"><br><br>
                            <input class="textbox" id="password" placeholder="password" type="password"><br>
                            <br>
                            <input class="button" type="submit" id="loginButton" value="Login">
                            <input class="button" type="submit" value="Signup">
                        </div>
                    </div>
                </div>
                
                <br><br>


                <div id="suggestionPane"></div>
                <br><br><br><br>
            </div>


        
            <div id="rightPane" style="float:left;width:60%;margin-left:10%">        
                <div id="subtopicPane"></div><br><br>
            </div>
            

        </div>
    </body>

</html>

<script type="text/javascript">
    topic="supertopic"
</script>

 <% 
 

 if(typeof domain != 'undefined'){ %>
    <script type="text/javascript">
        topic='<%= domain%>';
    </script>
<% } %>

<script type="text/javascript" src="/static/contentManager.js"></script>



